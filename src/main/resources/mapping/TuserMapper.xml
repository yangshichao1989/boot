<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.TuserDao">

	<resultMap id="BaseResultMap" type="com.example.demo.model.Tuser" >
   		 <id column="id" property="id" jdbcType="INTEGER" />
   		 <result column="name" property="name" jdbcType="VARCHAR" />
   		 <result column="password" property="password" jdbcType="VARCHAR" />
  	</resultMap>

	<select id="getAllUser" resultType="com.example.demo.model.Tuser">
		select * from tuser
	</select>
	
	<insert id="seve" parameterType="com.example.demo.model.Tuser" >
   		 insert into tuser (id,name, password)
  				  values (#{id}, #{name}, #{password})
 	 </insert>

</mapper>
